{
  "name": "cssxe",
  "version": "0.1.0",
  "description": "A tool for live editing and syncing CSS directly from the browser to your source files",
  "main": "index.jsx",
  "scripts": {
    "start": "NODE_ENV=production node server/server.js",
    "build": "vite build",
    "dev": "concurrently \"vite\" \"NODE_ENV=development nodemon server/server.js\"",
    "preview": "vite preview",
    "presexy": "chmod +x ./client/features/CDP/startRemoteChrome.sh",
    "sexy": "./client/features/CDP/startRemoteChrome.sh"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/cssxe.git"
  },
  "keywords": [
    "CSS",
    "live-edit",
    "web-development",
    "tooling",
    "devtools",
    "workspace",
    "source-maps"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.1",
    "chrome-remote-interface": "^0.33.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "puppeteer": "^22.3.0",
    "react-redux": "^9.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.1.4"
  }
}
