{
  "name": "cssxe",
  "version": "0.1.0",
  "description": "A tool for live editing and syncing CSS directly from the browser to your source files",
  "main": "index.jsx",
  "scripts": {
    "start": "NODE_ENV=production node server/server.js",
    "build": "vite build",
    "preview": "vite preview --port 8888",
    "vite-dev": "concurrently \"vite\" \"NODE_ENV=development nodemon server/server.js\"",
    "dev": "port=5555 npm run startRemoteChrome && npm run vite-dev",
    "prod": "npm run build && port=8888 npm run startRemoteChrome && npm run start",
    "startRemoteChrome": "PORT=$port ./scripts/startRemoteChrome.sh",
    "postInstall": "node ./scripts/postInstall.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/cssxe.git"
  },
  "bin": {
    "remoteChrome": "./startRemoteChrome.sh"
  },
  "keywords": [
    "CSS",
    "live-edit",
    "web-development",
    "tooling",
    "devtools",
    "workspace",
    "source-maps"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.1",
    "base-64": "^1.0.0",
    "chrome-remote-interface": "^0.33.0",
    "cors": "^2.8.5",
    "decode-base64": "^3.0.1",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "msgpack-lite": "^0.1.26",
    "puppeteer": "^22.4.0",
    "react-redux": "^9.1.0",
    "url": "^0.11.3",
    "utf8": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.1.4"
  }
}
